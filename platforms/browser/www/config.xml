<?xml version='1.0' encoding='utf-8'?>
<widget id="com.techstreet.sample" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>Sample</name>
    <description>
        A Sample PhoneGap app.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="icon.png" />
    <splash src="splash.png" />
    <preference name="phonegap-version" value="cli-8.1.1" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="StatusBarBackgroundColor" value="#A9A9A9" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
